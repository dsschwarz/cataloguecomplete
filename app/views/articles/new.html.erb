<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for @article do |f| %>

      <%= f.label :picture, text: "Link to picture" %>
      <%= f.text_field :picture %>

      <%= f.submit "Done", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>